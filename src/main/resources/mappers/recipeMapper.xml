<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.allured.yourRecipe.recipe.dao.RecipeDAO">
	<insert id="insertRecipe" parameterType="map">
		INSERT INTO
			`recipe`
		(
			`userNickname`
			, `title`
			, `content`
			, `imgUrl`
			, `createdAt`
			, `updatedAt`
		) VALUES
		(
			#{userNickname}
			, #{title}
			, #{content}
			, #{imgUrl}
			, now()
			, now()
		)
	</insert>
</mapper>